{
    "root": "public",
    "clean_urls": true,
    "https_only": true,
    "error_page": "error.html",
    "headers": {
        "/**": {
            "Access-Control-Allow-Origin": "*"
        },
        "/.well-known/apple-app-site-association": {
            "Content-Type": "application/pkcs7-mime"
        }
    }
}